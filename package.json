{
  "name": "placements-api",
  "version": "1.0.0",
  "description": "Placements API for Anna University - CUIC",
  "main": "src/server.js",
  "scripts": {
    "rollback": "knex migrate:rollback --knexfile ./src/knexfile.js",
    "migrate": "knex migrate:latest --knexfile ./src/knexfile.js",
    "seed": "knex seed:run --knexfile ./src/knexfile.js",
    "knex:dev": "npm run migrate && npm run seed",
    "dev": "nodemon --exec babel-node ./src/server.js",
    "dev:start": "npm run knex:dev && npm run dev",
    "clean": "rm -rf ./dist",
    "build": "babel ./src -d dist --copy-files",
    "start": "node ./dist/server.js",
    "rollback:prod": "knex migrate:rollback --knexfile ./dist/knexfile.js",
    "migrate:prod": "knex migrate:latest --knexfile ./dist/knexfile.js",
    "seed:prod": "knex seed:run --knexfile ./dist/knexfile.js",
    "knex:prod": "npm run migrate:prod && npm run seed:prod",
    "build:start": "npm run clean && npm run build && npm run knex:prod && npm run start",
    "heroku-postbuild": "npm run build && npm run knex:prod"
  },
  "author": "Mugilan <mugilancodes@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "googleapis": "^73.0.0",
    "helmet": "^4.2.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@types/jsonwebtoken": "^8.5.0",
    "nodemon": "^2.0.6",
    "ntl": "^5.1.0"
  }
}
